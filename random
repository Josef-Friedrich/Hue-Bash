#! /bin/sh



hue_random_range() {



  local START=${1%%:*}
  local END=${1#*:}

  local END=$((END + 1))

  local RANGE=$((END - START))

  RANDOM="1$RANDOM"

  local NUMBER_IN_RANGE=$((RANDOM % RANGE))

  echo $((NUMBER_IN_RANGE + START))
}

